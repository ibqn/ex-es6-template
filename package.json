{
  "name": "ex-es6-template",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src",
    "clean": "rimraf dist",
    "start": "concurrently --names 'babel,bs' --prefix-colors \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" 'nodemon --watch src --exec \"DEBUG=ex-es6-template:* babel-node src\"' 'lite-server'",
    "build": "babel src --copy-files --out-dir dist",
    "serve": "concurrently \"node dist\" \"lite-server\""
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "node-sass-middleware": "^0.11.0",
    "pug": "^2.0.0-rc.4",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.12.1",
    "http-proxy-middleware": "^0.17.4",
    "lite-server": "^2.3.0",
    "nodemon": "^1.12.1"
  }
}
